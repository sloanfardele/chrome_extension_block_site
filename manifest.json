{
  "manifest_version": 2,

  "name": "Safe Browsing",
  "description": "Boost de motivation",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "optional_permissions": [
    "webRequest",
    "webRequestBlocking"
  ]
}